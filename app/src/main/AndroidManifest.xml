        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".first_demo.SwipeAndViewActivity"></activity>

        <activity android:name=".first_demo.RelativeLayout">
